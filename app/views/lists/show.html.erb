<h2><%= @list.name %></h2>
<ul class="mt-3">
  <% @list.bookmarks.each do |bookmark| %>
  <li>
    <%= bookmark.movie.title %>
     <%= link_to "delete",
    bookmark_path(bookmark),
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
  %>
  </li>
  <% end %>
</ul>


<%= link_to '< Back', lists_path, class:"btn btn-secondary rounded-pill" %>
<%= link_to 'New Bookmark', new_list_bookmark_path(@list), class:"btn btn-secondary rounded-pill" %>
